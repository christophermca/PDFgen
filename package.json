{
  "name": "PDFgen",
  "version": "1.0.0",
  "description": "HTML to PDF Generator",
  "main": "./server/index.js",
  "repository": "git@github.com:christophermca/PDFgen.git",
  "author": "Christopher MCAdams <mca.christopher@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "d3": "^4.0.0",
    "express": "^4.16.4",
    "expressjs": "^1.0.1",
    "headless-chrome": "^0.0.0",
    "jsdom": "^13.1.0",
    "mocha": "^5.2.0",
    "mustache": "^3.0.1",
    "mustache-express": "^1.2.8",
    "puppeteer": "^1.11.0",
    "sass": "^1.16.1",
    "scss": "^0.2.4",
    "sinon": "^7.2.2"
  },
  "scripts": {
    "build": "rm -rf dist && npm run build-css && babel src -d dist \"$@\" --copy-files",
    "build-css": "sass src/generator/templates/main.scss dist/generator/templates/main.css",
    "watch": "npm run build --watch ",
    "start": "npm run build && node ./dist/server/index.js",
    "debug": "npm run build && node inspect ./dist/server/index.js",
    "clean-tmp": "rm -rf ./tmp/*.pdf",
    "test": "./node_modules/.bin/mocha --reporter spec",
    "stop": "pkill node"
  }
}
